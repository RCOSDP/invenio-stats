{
  "index_patterns": ["__SEARCH_INDEX_PREFIX__stats-item-create-*"],
  "settings": {
    "index.mapper.dynamic": false,
    "index": {
      "refresh_interval": "1m"
    }
  },
  "mappings": {
    "item-create-day-aggregation": {
      "date_detection": false,
      "dynamic": false,
      "numeric_detection": false,
      "properties": {
        "timestamp": {
          "type": "date",
          "format": "date_optional_time"
        },
        "unique_id": {
          "type": "keyword",
          "index": true
        },
        "count": {
          "type": "integer"
        },
        "unique_count": {
          "type": "integer"
        },
        "country": {
          "type": "keyword",
          "index": true
        },
        "hostname": {
          "type": "keyword",
          "index": true
        },
        "remote_addr": {
          "type": "keyword",
          "index": true
        },
        "collection": {
          "type": "keyword",
          "index": true
        }
      }
    },
    "item-create-agg-bookmark": {
      "date_detection": false,
      "properties": {
        "date": {
          "type": "date",
          "format": "date_optional_time"
        }
      }
    }
  },
  "aliases": {
    "stats-item-create": {}
  }
}
