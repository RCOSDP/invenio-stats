{
  "index_patterns": ["__SEARCH_INDEX_PREFIX__stats-record-view-*"],
  "settings": {
    "index.mapper.dynamic": false,
    "index": {
      "refresh_interval": "1m"
    }
  },
  "mappings": {
    "record-view-day-aggregation": {
      "date_detection": false,
      "dynamic": false,
      "numeric_detection": false,
      "properties": {
        "timestamp": {
          "type": "date",
          "format": "date_optional_time"
        },
        "count": {
          "type": "integer"
        },
        "record_id": {
          "type": "keyword",
          "index": true
        },
        "collection": {
          "type": "keyword",
          "index": true
        }
      }
    }
  },
  "aliases": {
    "stats-record-view": {}
  }
}
