{
  "template": "events-stats-item-create-*",
  "settings": {
    "index.mapper.dynamic": false,
    "index": {
      "refresh_interval": "5s"
    }
  },
  "mappings": {
    "stats-item-create": {
      "_all": {
        "enabled": false
      },
      "date_detection": false,
      "dynamic": false,
      "numeric_detection": false,
      "properties": {
        "timestamp": {
          "type": "date",
          "format": "strict_date_hour_minute_second"
        },
        "referrer": {
          "type": "string",
          "index": "not_analyzed"
        },
        "remote_addr": {
          "type": "string",
          "index": "not_analyzed"
        },
        "pid_type": {
          "type": "string",
          "index": "not_analyzed"
        },
        "pid_value": {
          "type": "string",
          "index": "not_analyzed"
        }
      }
    }
  },
  "aliases": {
    "events-stats-item-create": {}
  }
}
